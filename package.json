{
  "name": "electron-shirtastic-app",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-shirtastic-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=DEV6 --version-string.FileDescription=DEV6 --version-string.ProductName=\"ElectronShirtasticApp\"",
    "package-linux": "electron-packager . electron-shirtastic-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/512x512.png --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "4.0.1",
    "electron-packager": "^12.1.0"
  },
  "dependencies": {
    "electron-reload": "^1.4.0",
    "axios": "^0.18.0",
    "json-server": "^0.14.2"
  }
}
